		.portfolio-item.grid(style="background-image: url(../img/3.jpg)")
			span Cool iMac
		.portfolio-item.grid(style="background-image: url(../img/4.jpg)")
			span Cool Hacking
		.portfolio-item.grid(style="background-image: url(../img/5.jpg)")
			span Cool Bull Shit
		.portfolio-item.grid(style="background-image: url(../img/6.jpg)")
			span Cool WorkSpace
		.portfolio-item.grid(style="background-image: url(../img/7.jpg)")
			span Cool Notebook
		.portfolio-item.grid(style="background-image: url(../img/8.jpg)")
			span Cool Code
		.portfolio-item.grid(style="background-image: url(../img/9.jpg)")
			span Cool Timer
		.buttoncase.text-center
